<configuration>
  <system.web>
    <compilation targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <appSettings>    
    <add key="PassportSiteAddress" value="http://www.Passport.com/Login.aspx"/>
    <!--<add key="PassportSiteAddress" value="http://localhost:55259/Login.aspx"/>-->
  </appSettings>
  <system.webServer>
    <defaultDocument>
      <files>
        <add value="webform1.aspx"/>
      </files>
    </defaultDocument>
  </system.webServer>


  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="PassportWebServiceSoap" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://www.passport.com/PassportWebService.asmx"
          binding="basicHttpBinding" bindingConfiguration="PassportWebServiceSoap"
          contract="PassportServiceReference.PassportWebServiceSoap"
          name="PassportWebServiceSoap" />
    </client>
  </system.serviceModel>
</configuration>
